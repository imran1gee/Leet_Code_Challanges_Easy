   "source": [
    "def Fun(nums):\n",
    "    zero_index = 0  # Index to keep track of the position to swap zeros\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i] != 0:\n",
    "            nums[i], nums[zero_index] = nums[zero_index], nums[i]\n",
    "            zero_index += 1\n",
    "    return nums\n",
    "nums = [1,4,0,3,5,0,4]\n",
    "Fun(nums)"
   ]
