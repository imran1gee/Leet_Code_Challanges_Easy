   "source": [
    "def Fun(x):\n",
    "    if x==0 or x== 1:\n",
    "        return 1\n",
    "    d = [0]*(x+1)\n",
    "    d[0],d[1] = 1,1\n",
    "    for i in range(2,x+1):\n",
    "        d[i] = d[i-2]+d[i-1]\n",
    "    return d[i]"
   ]
